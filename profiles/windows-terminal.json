{
	"$schema": "https://aka.ms/terminal-profiles-schema",
	"defaultProfile": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
	"disabledProfileSources": [
		"Windows.Terminal.Wsl",
		"Windows.Terminal.Azure", 
		"Windows.Terminal.PowershellCore"
	],
	"profiles": {
		"defaults": {
			"cursorColor": "#7C6F64",
			"cursorShape": "filledBox",
			"fontFace": "Cascadia Mono",
			"fontSize": 10,
			"hidden": true,
			"padding": "0,0,0,0",
			"antialiasingMode": "cleartype",
			"colorScheme": "Gruvbox Dark",
			"startingDirectory": "",
			"bellStyle": "none"
		},
		"list": [
			{
				"guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
				"hidden": false,
				"name": "cmd",
				"commandline": "cmd /K %HOMEDRIVE%\\%HOMEPATH%\\cmdrc.bat",
				"startingDirectory": "%HOMEPATH%"
			},
			{
				"hidden": true,
				"name": "nu",
				"commandline": "%HOMEPATH%/scoop/apps/nu/current/nu.exe",
				"startingDirectory": "%HOMEPATH%"
			},
			{
				"hidden": true,
				"name": "shell",
				"commandline": "%HOMEPATH%/scoop/apps/busybox/current/busybox.exe bash -s -c \"source ~/.profile\"",
				"startingDirectory": "%HOMEPATH%"
			},
			{
				"hidden": true,
				"name": "verco",
				"commandline": "verco"
			}
		]
	},
	"copyOnSelect": false,
	"copyFormatting": false,
	"useTabSwitcher": "disabled",
	"windowingBehavior": "useExisting",
	"confirmCloseAllTabs": true,
	"disableAnimations": true,
	"initialCols": 120,
	"initialRows": 50,
	"initialPosition": "0,0",
	"launchMode": "maximized",
	"actions": [
		{ "command": null, "keys": "ctrl+," },
		{ "command": null, "keys": "ctrl+shift+," },
		{ "command": null, "keys": "ctrl+shift+c" },
		{ "command": null, "keys": "ctrl+shift+v" },
		{ "command": null, "keys": "ctrl+shift+n" },
		{ "command": "newTab", "keys": "ctrl+shift+t" },
		{ "command": "closePane", "keys": "ctrl+shift+w" },
		{ "command": { "action": "copy", "singleLine": false }, "keys": "ctrl+c" },
		{ "command": "paste", "keys": "ctrl+v" },
		{ "command": { "action": "splitPane", "split": "auto", "splitMode": "duplicate" }, "keys": "shift+tab" },
		{ "command": { "action": "moveFocus", "direction": "down" }, "keys": "ctrl+shift+j" },
		{ "command": { "action": "moveFocus", "direction": "left" }, "keys": "ctrl+shift+h" },
		{ "command": { "action": "moveFocus", "direction": "right" }, "keys": "ctrl+shift+l" },
		{ "command": { "action": "moveFocus", "direction": "up" }, "keys": "ctrl+shift+k" },
		{ "command": { "action": "sendInput", "input": "\u000d" }, "keys": "ctrl+m" },
		{ "command": { "action": "sendInput", "input": "\u007f" }, "keys": "ctrl+h" }
	],
	"schemes": [
		{
			"name": "Gruvbox Dark",
			"background": "#282828",
			"foreground": "#EBDBB2",
			"black": "#282828",
			"blue": "#458588",
			"brightBlack": "#928374",
			"brightBlue": "#83A598",
			"brightCyan": "#8EC07C",
			"brightGreen": "#B8BB26",
			"brightPurple": "#D3869B",
			"brightRed": "#FB4934",
			"brightWhite": "#EBDBB2",
			"brightYellow": "#FABD2F",
			"cyan": "#689D6A",
			"green": "#98971A",
			"purple": "#B16286",
			"red": "#CC241D",
			"white": "#A89984",
			"yellow": "#D79921"
		}
	]
}
