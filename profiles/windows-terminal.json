		"list": [
			{
				"guid": "{574e775e-4f2a-5b96-ac1e-a2962a402336}",
				"hidden": false,
				"name": "shell",
				"commandline": "%HOMEPATH%/scoop/apps/busybox/current/busybox.exe bash -l"
			}
		]
	},
	"copyOnSelect": false,
	"copyFormatting": false,
	"schemes": [
		{
			"name": "Gruvbox Dark",
			"background": "#282828",
			"foreground": "#EBDBB2",
			"black": "#282828",
			"blue": "#458588",
			"brightBlack": "#928374",
			"brightBlue": "#83A598",
			"brightCyan": "#8EC07C",
			"brightGreen": "#B8BB26",
			"brightPurple": "#D3869B",
			"brightRed": "#FB4934",
			"brightWhite": "#EBDBB2",
			"brightYellow": "#FABD2F",
			"cyan": "#689D6A",
			"green": "#98971A",
			"purple": "#B16286",
			"red": "#CC241D",
			"white": "#A89984",
			"yellow": "#D79921"
		}
	],
	"keybindings": [
		{
			"command": null,
			"keys": "ctrl+,"
		},
		{
			"command": "openSettings",
			"keys": "ctrl+shift+,"
		},
		{
			"command": null,
			"keys": "ctrl+shift+c"
		},
		{
			"command": null,
			"keys": "ctrl+shift+v"
		},
		{
			"command": "newTab",
			"keys": "ctrl+shift+t"
		},
		{
			"command": "closePane",
			"keys": "ctrl+shift+w"
		},
		{
			"command": {
				"action": "copy",
				"singleLine": false
			},
			"keys": "ctrl+c"
		},
		{
			"command": "paste",
			"keys": "ctrl+v"
		},
		{
			"command": {
				"action": "splitPane",
				"split": "auto",
				"splitMode": "duplicate"
			},
			"keys": "shift+tab"
		},
		{
			"command": {
				"action": "splitPane",
				"split": "horizontal",
				"commandline": "%HOMEPATH%/scoop/apps/busybox/current/busybox.exe bash -c \"cd $(cat ~/pepperdir); fzf | xargs -rI FILE pepper --as-focused-client \\\"FILE\\\"\"; rm ~/pepperdir"
			},
			"keys": "ctrl+shift+alt+p"
		}
	],
	"initialCols": 120,
	"initialRows": 50,
	"initialPosition": "0,0",
	"launchMode": "maximized"
